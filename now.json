{
    "name": "hospice-hope-contemplating-action",
    "scope": "ca-hospice-hope",
    "builds": [
        {
            "src": "dist/**/*",
            "use": "@now/static"
        }
    ],
    "routes": [
        {
            "src": "/dist/.*",
            "status": 404,
            "dest": "nothing"
        },
        {
            "src": "/(.*)",
            "dest": "/dist/$1"
        }
    ]
}